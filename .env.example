# Umami Analytics Configuration
# Copy this file to .env and fill in your values

# Umami server URL (your private deployment)
NUXT_PUBLIC_UMAMI_HOST=https://umami-rose-delta.vercel.app

# Website ID from Umami dashboard
# Dev/Preview: a56d992c-093a-4c58-aaae-59cf5d2cdc0d
# Production:  96586b14-00ab-43d1-8c31-3212d9f9beee
NUXT_PUBLIC_UMAMI_ID=a56d992c-093a-4c58-aaae-59cf5d2cdc0d

# Google Tag Manager Configuration
# Only loaded in production environment (baseURL: /bookleaf/)
# Production GTM ID: TM-WMJSMTWF
NUXT_PUBLIC_GTM_ID=TM-WMJSMTWF

# Base URL Configuration
# - Local dev / Vercel: leave empty or don't set (defaults to /)
# - Spring Boot production: set to /bookleaf/
# NUXT_APP_BASE_URL=/bookleaf/

# Google Tag Manager (production only)
# Only set this in production environment
# NUXT_PUBLIC_GTM_ID=GTM-XXXXXXX
